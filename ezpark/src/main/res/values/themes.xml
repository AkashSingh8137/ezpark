    <!-- Base application theme. -->
    <style name="Theme.Ezpark" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targatApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->

    </style>
    <color name="orange">#fc9803</color>
    <color name="darkblue">#2403fc</color>
    <color name="red">#F60000</color>
<style name="SplashScreen" parent="Theme.MaterialComponents.DayNight.NoActionBar">  <!-- prints the splash screen . -->
    <item name="android:windowBackground">@drawable/background_splashscreen</item>
    <item name="android:statusBarColor" tools:targatApi="l">?attr/colorPrimary</item>
</style>
</resources>